{
    "name": "runopencode/bitmask",
    "description": "Provides possibility to work with bit masks and to store them into database using doctrine/dbal.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@runopencode.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-bitset": "*",
        "doctrine/dbal": "^4.0",
        "thecodingmachine/safe": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Component\\Bitmask\\": "src"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Component/Dataset",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Component/Query",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Component/Metadata",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Bundle/MetadataBundle",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Bundle/QueryBundle",
            "options": {
                "symlink": false
            }
        }
    ],
    "config": {
        "sort-packages": true
    },
    "version": "0.2",
    "minimum-stability": "dev",
    "prefer-stable": true
}
